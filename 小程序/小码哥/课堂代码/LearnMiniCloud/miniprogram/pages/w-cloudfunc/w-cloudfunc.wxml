<!--pages/w-cloudfunc/w-cloudfunc.wxml-->
<!-- 1.基本使用：通过云函数对两个数字进行相加 -->
<button bindtap="basicUsing">云函数的基本使用</button>

<!-- 2.通过云函数获取openid -->
<button bindtap="getOpenID">获取OpenID</button>

<!-- 3.通过云函数删除多条数据 -->
<!-- 
  云函数操作数据库权限更大：
    1.删除、更新(update)、修改（set）,可以根据条件修改多条数据
    2.查询数据时，最多一次可以查询100条数据（小程序端一次最多可以查询20条数据）
 -->
<button bindtap="removeMultidata">删除多条数据</button>

<!-- 4.通过云调用生效小程序码 -->
<button bindtap="generateCode">生成小程序码</button>
<!-- <image src="https://636f-coderwhy-i7b7z-1259322030.tcb.qcloud.la/images/minicode.png?sign=516b6753ec8e4898695e0564dd6f6169&t=1575030459"/>
<image src="cloud://coderwhy-i7b7z.636f-coderwhy-i7b7z-1259322030/images/minicode.png"></image> -->

<!-- 5.发送模板消息: formid -->
<form report-submit="true" bindsubmit="formSubmit">
  <button form-type="submit">提交表单</button>
</form>
